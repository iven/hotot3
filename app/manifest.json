{
    "name": "__MSG_appName__",
    "description": "__MSG_appDescription__",
    "version": "3.0.18.6",
    "minimum_chrome_version": "27",
    "manifest_version": 2,
    "default_locale": "en",
    "app": {
        "background": {
            "scripts": [
                "scripts/main.js",
                "scripts/chromereload.js"
            ]
        }
    },
    "icons": {
        "16": "images/icons/22x22/apps/hotot.png",
        "24": "images/icons/24x24/apps/hotot.png",
        "128": "images/icons/128x128/apps/hotot.png"
    },
    "permissions": [
        "clipboardRead",
        "clipboardWrite",
        "contextMenus",
        {
            "fileSystem": [
                "write",
                "retainEntries",
                "directory"
            ]
        },
        "notifications",
        "storage",
        "unlimitedStorage",
        "webview",
        "http://*/*",
        "https://*/*"
    ]
}
